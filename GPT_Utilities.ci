INT
FUNCTION DspGetMouseOverPrevious(INT iNext=0)
INT i;
INT iAN,iRETURN;
	
	iAN = DspGetAnCur();

	IF iNext >= 0 THEN
		FOR i = 1 TO iNext DO
			iAN = DspGetAnPrevious(iAN);
			IF iAN = -1 THEN

				RETURN 0;
			END
		END
	END

	iRETURN = DspGetMouseOver(iAN);

	RETURN iRETURN;
END

