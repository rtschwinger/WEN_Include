STRING 
FUNCTION GetHtmlPath(STRING sPath)
INT i,j;
INT iPathLen=StrLength(sPath);
STRING s;
STRING sReturn;

	FOR i = 1 TO iPathLen DO
		s = StrMid(sPath,iPathLen-i,1);
		IF s = "\" THEN
			j = i - 1;
			i = iPathLen;
		END
	END
	
	sReturn = StrLeft(sPath,iPathLen - j);

	RETURN sReturn;
END

