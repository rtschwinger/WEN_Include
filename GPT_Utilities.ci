   hFieldName[2]= FormField(2, 3, 30, 1, 3, sProjWENMaVer, "Project Major Version:  ", 0);
   hFieldName[3]= FormField(2, 4, 30, 1, 3, sProjWENMiVer, "Project Minor Version:  ", 0);
