INT
FUNCTION OpenDayTankDetail(INT DayTankNum)
	!Dim
	STRING dtpage1 = "CHG3_FOCS_DT1_4";
	STRING dtpage2 = "CHG3_FOCS_DT5_8";
	STRING dtpage3 = "CHG3_FOCS_DT9_12";
	STRING dtpage4 = "CHG3_FOCS_DT13_16";
	
	! Get Current Day Tank Number passed in by Genie
	! Open correct page based on Day Tank clicked on
	SELECT CASE DayTankNum 
		CASE 1
			PageDisplay(dtpage1);
		CASE 2	
			PageDisplay(dtpage1);
		CASE 3
			PageDisplay(dtpage1);
		CASE 4
			PageDisplay(dtpage1);
		CASE 5
			PageDisplay(dtpage2);
		CASE 6
			PageDisplay(dtpage2);
		CASE 7
			PageDisplay(dtpage2);
		CASE 8
			PageDisplay(dtpage2);
		CASE 9
			PageDisplay(dtpage3);
		CASE 10
			PageDisplay(dtpage3);
		CASE 11
			PageDisplay(dtpage3);
		CASE 12
			PageDisplay(dtpage3);
		CASE 13
			PageDisplay(dtpage4);
		CASE 14
			PageDisplay(dtpage4);
		CASE 15
			PageDisplay(dtpage4);
		CASE 16
			PageDisplay(dtpage4);
	END SELECT
	
	RETURN 0
END

