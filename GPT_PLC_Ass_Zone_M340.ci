//********************** PLC M340 DIO **********************/
//Global Power  RTS 2016/08/12 New
//********************** PLC M340 Msg **********************
FUNCTION M340_ASS_DIO(STRING strDevice,STRING strCluster,STRING sZone="Z1",STRING sRack="1",STRING sSlot="1",STRING sType="DDI",STRING sRev1="3202",STRING sRev2="K")
	STRING sEquip,sTitle;
	STRING sType1,sType2,sType3;
	
	//Init
	IF strCluster <> "" THEN strCluster = strCluster + "."; END
	//------------------------------------------------------
	//                   Equip Ass
	//------------------------------------------------------
	Ass(-2,"Cluster","'" + strCluster + "'",0);
	Ass(-2,"Type","'" + sType + "'",0);
	Ass(-2,"Equip","'" + strDevice + "'",0);
	Ass(-2,"Title","'" + "Rack " + sRack + ",Slot " + sSlot + "'",0);
	//------------------------------------------------------
	//                   Type Ass
	//------------------------------------------------------
	Ass(-2,"Type1","'" + "BMX" + "'",0);
	Ass(-2,"Type2","'" + sType + "" + sRev1 + "" + sRev2 + "'",0);
	IF sType = "DDI" THEN Ass(-2,"Type3","'" + "DIGITAL INPUT MODULE" + "'",0); END
	IF sType = "DDO" THEN Ass(-2,"Type3","'" + "DIGITAL OUTPUT MODULE" + "'",0); END
	//------------------------------------------------------
	//             Variable and Msg Ass
	//------------------------------------------------------
	M340_ASS_TAG(strDevice,strCluster,sRack,sSlot,sType,sRev1,sRev2);
	M340_ASS_MSG(strDevice,strCluster,sRack,sSlot,sType,sRev1,sRev2);
	//------------------------------------------------------
	//                   Equip PopUp
	//------------------------------------------------------
	EquipAssMetadataWin("!PLC_DIO_Popup");
	WinTitle(AssInfo("4",0));
END

//********************** PLC M340 Tag M340_ASS_TAG **********************

FUNCTION M340_ASS_TAG(STRING strTAG,STRING sCluster,STRING sRack="1",STRING sSlot="1",STRING sType="DDI",STRING Rev1="",STRING Rev2="")
	INT i, session, status, numRecs;
	STRING sItem;
		
	FOR i = 0 TO 31 DO
		sItem = sCluster + strTAG + "\GGIO" + sRack + "\SLOT" + sSlot + "\" + sType + "[" + IntToStr(i) + "]";
		IF iDebug_On THEN
		   ErrLog(" PLC M340 sItem: " +  sItem);
		END	
		Ass(-2,"Tag" + IntToStr(i),sItem,0);
	END
END

//********************** PLC M340 Message M340_ASS_MSG **********************

FUNCTION M340_ASS_MSG(STRING strTAG,STRING sCluster,STRING sRack="1",STRING sSlot="1",STRING sType="DDI",STRING Rev1="",STRING Rev2="")
		STRING sFunction,sArgs;

		SELECT CASE strTAG
					//Medium Voltage Monitoring PLCs
					CASE "MDC_B1_M340", "MDC_B2_M340", "MDC_B3_M340", "SDC_B1_1_M340","SDC_B1_2_M340"
							sFunction = strTAG + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = "";
					//IR PMDC Monitoring PLCs
					CASE "PMDC_B1_1_i_M340","PMDC_B1_2_i_M340"
							sFunction = strTAG + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = "";
					//Mechanical PMDC Monitoring PLCs
					CASE "PMDC_B1_1_q_M340","PMDC_B1_1_r_M340","PMDC_B1_1_s_M340","PMDC_B1_2_q_M340","PMDC_B1_2_r_M340","PMDC_B1_2_s_M340"
							sFunction = strTAG + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = "";
					//Zone 1 Critical PMDC Monitoring PLCs
					CASE "PMDC_B1_1_a_M340","PMDC_B1_2_a_M340","PMDC_B1_1_b_M340","PMDC_B1_2_b_M340","PMDC_B1_1_c_M340","PMDC_B1_2_c_M340","PMDC_B1_1_d_M340"
							sFunction = "PMDC_M340_Z1" + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = strTAG;
					//Zone 2 Critical PMDC Monitoring PLCs
					CASE "PMDC_B1_1_e_M340","PMDC_B1_2_e_M340","PMDC_B1_1_f_M340","PMDC_B1_2_f_M340","PMDC_B1_1_g_M340","PMDC_B1_2_g_M340","PMDC_B1_2_d_M340"
							sFunction = "PMDC_M340_Z3" + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = strTAG;
					//Zone 3 Critical PMDC Monitoring PLCs
					CASE "PMDC_B1_1_n_M340","PMDC_B1_2_n_M340","PMDC_B1_1_o_M340","PMDC_B1_2_o_M340","PMDC_B1_1_p_M340","PMDC_B1_2_p_M340","PMDC_B1_2_m_M340"
							sFunction = "PMDC_M340_Z3" + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = strTAG;
					//Zone 4 Critical PMDC Monitoring PLCs
					CASE "PMDC_B1_1_h_M340","PMDC_B1_2_h_M340","PMDC_B1_1_j_M340","PMDC_B1_2_j_M340","PMDC_B1_1_k_M340","PMDC_B1_2_k_M340","PMDC_B1_1_m_M340"
							sFunction = "PMDC_M340_Z3" + "_Rack" + sRack + "_Slot" + sSlot + "_Msg";
							sArgs = strTAG;
					//Zone 5 Critical PMDC Monitoring PLCs
					CASE "PMDC_B3_1_a_M340","PMDC_B3_1_b_M340","PMDC_B3_1_c_M340","PMDC_B3_1_d_M340","PMDC_B3_2_a_M340","PMDC_B3_2_b_M340","PMDC_B3_2_c_M340"
							sFunction = "M340_ASS_POPUP_MSG";
							sArgs = strTAG + "," + sSlot + "," + sSlot;
					//Zone 6 Critical PMDC Monitoring PLCs
					CASE "PMDC_B3_2_d_M340","PMDC_B3_2_e_M340","PMDC_B3_2_f_M340","PMDC_B3_2_g_M340","PMDC_B3_1_e_M340","PMDC_B3_1_f_M340","PMDC_B3_1_g_M340"
							sFunction = "M340_ASS_POPUP_MSG";
							sArgs = strTAG + "," + sSlot + "," + sSlot;
					//Zone 7 Critical PMDC Monitoring PLCs
					CASE "PMDC_B3_1_h_M340","PMDC_B3_1_j_M340","PMDC_B3_1_k_M340","PMDC_B3_1_m_M340","PMDC_B3_2_h_M340","PMDC_B3_2_j_M340","PMDC_B3_2_k_M340"
							sFunction = "M340_ASS_POPUP_MSG";
							sArgs = strTAG + "," + sSlot + "," + sSlot;
					//Zone 5 SDC Monitoring PLCs
					CASE "SDC_B3_1_M340","SDC_B3_2_M340","MDC_B3_M340"
							sFunction = "M340_ASS_POPUP_MSG";
						strTAG = StrLeft(strTAG,8);
							sArgs = strTAG + "," + sSlot + "," + sSlot;
							ErrLog("Function is : " + sFunction + " Arguments : " + sArgs);
					//Zone 5 Mechanical PMDC Monitoring PLCs
					CASE "PMDC_B3_1_q_M340","PMDC_B3_1_r_M340","PMDC_B3_2_q_M340","PMDC_B3_2_r_M340","PMDC_B3_2_s_M340"
							sFunction = "M340_ASS_POPUP_MSG";
							sArgs = strTAG + "," + sSlot + "," + sSlot;
					//IR PMDC Monitoring PLCs
					CASE "PMDC_B3_1_i_M340","PMDC_B3_2_i_M340"
							sFunction = "M340_ASS_POPUP_MSG";
							sArgs = strTAG + "," + sSlot + "," + sSlot;
		END SELECT
		IF iDebug_On THEN
					ErrLog("M340 Taskcall Function: " + sFunction + " M340 Taskcall Args: " + sArgs);
		END

			TaskCall(sFunction, sArgs);
		END

//********************** PLC M340 Tag M340_ASS_POPUP_MSG **********************
INT
FUNCTION M340_ASS_POPUP_MSG(STRING sGearName,	STRING sSLOTNUM, STRING	sCOLNAME)
	INT	hDBFDevice;
	INT	iMsgCntr = 0;
	STRING sM340_IO_Descr;
	STRING sSLOTLBL = "SLOT" + sSLOTNUM;
	STRING sM340_IO_Location;
	STRING sM340_IO_Area;
	STRING sGear,sGear_DBF;
  ErrLog("M340 Gearname: " +  sGearName + " Slot: " + sSLOTNUM + " Colum: " + sCOLNAME);
	ErrSet(1);
	IF StrLeft(sGearName,4) = "PMDC" THEN
		sGear = StrLeft(sGearName,11);
		sGear_DBF = CharReplace("_","-",	sGear);
	ELSE
		sGear = StrLeft(sGearName,8);
		sGear_DBF = CharReplace("_","-",	sGear);
	END

	ErrLog("M340 Gearname: " +  sGearName + " Slot: " + sSLOTLBL + " Colum: " + sCOLNAME);

	hDBFDevice = DevOpen("PMDCM340Map",16); //Open	Device
	ErrLog("M340 DBF	Error: " + IntToStr(hDBFDevice))
	IF hDBFDevice	=	-1 THEN
		DspError("DBFDevice	Device Error!!");
		//TraceMsg("Unable to	Open DBF Device	");
		Message("Database	Access Error!", "Does someone have the file open?",	0);
		RETURN FALSE;
	ELSE
		WHILE	NOT	DevEOF(hDBFDevice) DO
			sM340_IO_Area = DevGetField(hDBFDevice,"AREAP");
			sM340_IO_Location = DevGetField(hDBFDevice,"LOCATION");
			sM340_IO_Descr = DevGetField(hDBFDevice,"DESCR");
			IF ((sGear_DBF = sM340_IO_Area) AND (sSLOTLBL = sM340_IO_Location)) THEN
		          ErrLog("------------------------------------------------");
		          ErrLog("String Returned: " + sM340_IO_Area);
		          ErrLog("String Returned: " + sM340_IO_Location);
		          ErrLog("String Returned: " + sM340_IO_Descr);
		          ErrLog("------------------------------------------------");
					    Ass(-2,"Msg" + IntToStr(iMsgCntr),"'"	+	 IntToStr(iMsgCntr)	+	".)	"	+	sGear_DBF	+	"	"	+	sM340_IO_Descr + "'",0);
					    ErrLog("String Assigned: " + IntToStr(iMsgCntr) + "'"	+	 IntToStr(iMsgCntr)	+	".)	"	+	sGear_DBF	+	"	"	+	sM340_IO_Descr);
					    iMsgCntr = iMsgCntr	+	1;
			END
	    DevNext(hDBFDevice);
			
		END	//WHILE	DO
		DevClose(hDBFDevice); //Close Device
	END
	DevClose(hDBFDevice);	//Close	Device
END

