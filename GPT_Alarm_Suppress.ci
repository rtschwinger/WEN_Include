STRING
FUNCTION SimpleTaskNew(STRING sFunName = "", STRING sArg1 = "", STRING sArg2 = "")
		STRING sFullArg;
		IF sArg2 = "" THEN
				sFullArg = sArg1;
		ELSE
				sFullArg = sArg1 + "," + sArg2;
		END
		RETURN TaskNew(sFunName,sFullArg,2);
END //FUNCTION SimpleTaskNew

